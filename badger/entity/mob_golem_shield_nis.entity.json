        "default": "geometry.mob_golem_shield_nis"
      },
      "identifier": "badger:mob_golem_shield_nis",
      "materials": {
        "default": "entity_model",
        "mat_mob_golem_shield": "mat_mob_golem_shield",
        "mat_mob_golem_shield_face": "mat_mob_golem_shield_face"
